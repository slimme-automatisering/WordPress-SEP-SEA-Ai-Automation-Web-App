{
  "name": "seo-sea-automation",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') >= 0) throw new Error('Please use npm to install dependencies')\"",
    "start": "node src/index.js",
    "start:prod": "NODE_ENV=production node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:frontend": "vite --config vite.frontend.config.js",
    "dev:dashboard": "vite --config vite.dashboard.config.js",
    "build": "npm run clean && npm run build:frontend && npm run build:dashboard",
    "clean": "rimraf dist",
    "prebuild:frontend": "mkdir -p dist/frontend",
    "prebuild:dashboard": "mkdir -p dist/dashboard",
    "build:frontend": "vite build --config vite.frontend.config.js --outDir dist/frontend --emptyOutDir",
    "build:dashboard": "vite build --config vite.dashboard.config.js --outDir dist/dashboard --emptyOutDir",
    "test": "jest",
    "test:unit": "jest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "test:e2e": "powershell ./scripts/run-e2e-tests.ps1",
    "test:e2e:local": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:load": "vitest run '__tests__/load/**/*.test.js'",
    "test:integration": "vitest run '__tests__/integration/**/*.test.js'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:verify": "cypress verify",
    "cypress:install": "cypress install",
    "lint": "eslint . --ext .js,.jsx",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "security:audit": "npm audit",
    "deps:check": "npm audit",
    "deps:update": "npm update"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "axios": "1.7.9",
    "chart.js": "^4.4.1",
    "cheerio": "1.0.0-rc.12",
    "compression": "^1.7.4",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "4.21.2",
    "express-csp-header": "^5.2.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.0",
    "express-session": "^1.17.3",
    "express-timeout-handler": "^2.2.0",
    "graceful-fs": "^4.2.11",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "7.8.3",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "6.9.16",
    "openai": "^4.24.1",
    "puppeteer": "^22.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uid-safe": "^2.1.5",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss-clean": "^0.1.4",
    "rimraf": "^5.0.5",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.13.9",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.8",
    "@types/cors": "2.8.17",
    "@types/jest": "^29.5.11",
    "@types/morgan": "1.9.9",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "cypress": "^13.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "junit-report-merger": "^7.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.0.4"
  }
}
